    %store/vec4 v0xff74a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xff73b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xff7570_0, 0, 1;
